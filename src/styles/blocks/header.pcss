.header {
    width: 100%;
    display: flex;
    padding-top: 65px;
    align-items: center;
    justify-content: space-between;
}
.header__logo {
    width: 55px;
    height: 30px;
    background:  svg-load('logo.svg', fill=$text-color, width=100%, height=100%) center center / cover no-repeat;
}
.logo {
    color: #fff;
    font-size: 36px;
    font-weight: 900;
}
.menu {
    flex: 1;
}
