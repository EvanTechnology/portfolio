.socials__list {
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.socials__item {
    margin-right: 40px;
    &:last-child {
        margin-right: 0px;
    }
}
.socials__icon {
  width: 30px;
  height: 30px;
  cursor: pointer;
  fill: #6f6b67;
  &:hover {
      fill: $orange
  }
}